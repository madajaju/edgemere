@startuml
  interface CLI
  interface Web
  interface WebSocket
package "Workflow Services" #lightblue {
  component express as REST
  component socket.io as socket
  folder bin #ffffff {
    component " edgemere aml ws" as binName
  }
  binName --> REST

  folder deploy #ffffff {
      file "node server.js\ndocker-compose.yml" as aml_ws_container
      REST --> aml_ws_container

  
    file "web" as web_container
    REST --> web_container
    socket ---> web_container
  
    file "gateway" as gateway_container
    REST --> gateway_container
    socket ---> gateway_container
  
  }
  folder images #ffffff {
    collections "aml_ws" as aml_wsimage
    artifact "ailtire:swarm" as ailtireswarm
    aml_wsimage <|-- ailtireswarm
    aml_ws_container --> aml_wsimage

  
      artifact aml_ws_web
      web_container --> aml_ws_web
    
      artifact aml_ws_gw
      gateway_container --> aml_ws_gw
    
  }
}
CLI " edgemere aml ws" -- binName
Web "/edgemere/aml/ws" - REST
WebSocket ".edgemere.aml.ws" -- socket

@enduml
